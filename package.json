{
  "name": "@primevoid/client",
  "version": "0.1.0",
  "description": "Next-generation JavaScript HTTP client focused on performance, security, and developer experience",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts --minify",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "test": "jest",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "prepublishOnly": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Prime-Void/client.git"
  },
  "keywords": [
    "http",
    "client",
    "fetch",
    "request",
    "axios",
    "typescript",
    "browser",
    "node"
  ],
  "author": "Prime Void Team",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Prime-Void/client/issues"
  },
  "homepage": "https://primevoid.com",
  "devDependencies": {
    "@types/jest": "^28.1.8",
    "@types/node": "^18.11.18",
    "@typescript-eslint/eslint-plugin": "^5.48.1",
    "@typescript-eslint/parser": "^5.48.1",
    "eslint": "^8.31.0",
    "jest": "^28.1.3",
    "prettier": "^2.8.2",
    "ts-jest": "^28.0.8",
    "tsup": "^6.5.0",
    "typescript": "^4.9.4"
  },
  "dependencies": {
    "abort-controller": "^3.0.0"
  },
  "engines": {
    "node": ">=18"
  },
  "publishConfig": {
    "access": "public"
  }
}